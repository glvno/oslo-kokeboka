{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"json-server --watch db.json\" \"vite\"",
    "preview": "vite preview",
    "bootstrap": "node ./scripts/bootstrap-fauna-database.ts",
    "start": "export SET NODE_OPTIONS=--openssl-legacy-provider && npm-run-all --parallel start:app start:server",
    "start:app": "vite",
    "start:server": "netlify-lambda serve functions",
    "build": "export SET NODE_OPTIONS=--openssl-legacy-provider && npm-run-all --parallel build:**",
    "build:app": "react-scripts build",
    "build:functions": "netlify-lambda build functions"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@netlify/functions": "^1.0.0",
    "axios": "^0.26.0",
    "faunadb": "^4.5.2",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.4",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-select": "^5.2.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "^14.18.12",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-react": "^1.0.7",
    "concurrently": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "json-server": "^0.17.0",
    "netlify-lambda": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2",
    "vite": "^2.8.0"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}